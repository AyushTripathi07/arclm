generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Notebook {
  id           String   @id @default(uuid())
  title        String
  icon         String
  date         String
  sources      Int
  highlight    Boolean
  content      String?
  lastModified DateTime?
}